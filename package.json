{
    "name": "portfolio",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "node src/server/server.js",
        "debug": "concurrently \"ng serve --proxy-config proxy.config.json\" \"nodemon src/server/server.js\"",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ng build --output-path dist",
        "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
        "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --format json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~10.1.4",
        "@angular/cli": "^10.1.4",
        "@angular/common": "~10.1.4",
        "@angular/compiler": "~10.1.4",
        "@angular/compiler-cli": "^10.1.4",
        "@angular/core": "~10.1.4",
        "@angular/forms": "~10.1.4",
        "@angular/platform-browser": "~10.1.4",
        "@angular/platform-browser-dynamic": "~10.1.4",
        "@angular/router": "~10.1.4",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "body-parser": "^1.19.0",
        "concurrently": "^5.3.0",
        "express": "^4.17.1",
        "mongodb": "^3.6.2",
        "nodemon": "^2.0.4",
        "rxjs": "^6.6.3",
        "tslib": "^2.0.2",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1001.4",
        "@angular/localize": "^10.1.4",
        "@biesbjerg/ngx-translate-extract": "^7.0.3",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.12.64",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}
